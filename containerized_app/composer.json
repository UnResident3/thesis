{
    "name": "thesis/php-sample-app",
    "description": "Sample containerized php webapp",
    "require": {
        "laravel/framework": "6.2",
        "laravel/tinker": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    }
}